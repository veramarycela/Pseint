algoritmo agua
 definir  n como numerico;
 definir kl como real;
 escribir "Introduzca el numero de segundos:";
 leer  n;

  kl<-( 245/1000)*n;

 escribir " Brotan de la fuentes, kl: ", kl;
    finalgoritmo